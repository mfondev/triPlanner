{
  "expo": {
    "name": "triPlanner",
    "slug": "triPlanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "triplaner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gideonatauba.triPlanner"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gideonatauba.triPlanner"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "iosLicenseKey": "\nU2NhbmJvdFNESwpjb20uZ2lkZW9uYXRhdWJhLnRyaVBsYW5uZXIKMTc2NDgwNjM5OQo4Mzg4NjA3CjE5\n",
      "androidLicenseKey": "\nU2NhbmJvdFNESwpjb20uZ2lkZW9uYXRhdWJhLnRyaVBsYW5uZXIKMTc2NDgwNjM5OQo4Mzg4NjA3CjE5\n",
      "router": {},
      "eas": {
        "projectId": "9872614b-e956-4656-817a-2132c0020d71"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "path/to/file.png",
            "path/to/directory"
          ]
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-asset",
      "expo-sqlite",
      "expo-notifications",
      [
        "react-native-scanbot-sdk",
        {
          "iOSCameraUsageDescription": "Credit card scanning permission",
          "largeHeap": true,
          "mavenURLs": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
